{
  "name": "@pascaliske/schematics",
  "description": "Angular schematics collection for integrating setup tools like prettier and storybook.",
  "version": "0.0.9",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/schematics",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/schematics/issues"
  },
  "keywords": [
    "angular",
    "schematics",
    "collection"
  ],
  "schematics": "./dist/collection.json",
  "scripts": {
    "watch": "webpack --config webpack.config.ts --progress --mode development --watch",
    "build": "webpack --config webpack.config.ts --progress --mode production",
    "lint": "tslint --project tsconfig.json --fix",
    "format": "prettier --write \"**/*.{ts,md}\"",
    "version": "bump --yes package.json",
    "changelog": "standard-changelog",
    "link": "yarn run build && yarn link",
    "unlink": "yarn unlink"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@pascaliske/prettier-config",
  "dependencies": {
    "@angular-devkit/core": "^10.1.7",
    "@angular-devkit/schematics": "^10.1.7",
    "@schematics/angular": "^10.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@pascaliske/prettier-config": "^1.1.0",
    "@pascaliske/tsconfig": "^1.0.4",
    "@types/node": "^14.11.8",
    "@types/webpack": "^4.41.22",
    "bump-cli": "^1.1.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "generate-json-webpack-plugin": "^1.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "rxjs-tslint-rules": "^4.34.3",
    "standard-changelog": "^2.0.24",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
