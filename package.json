{
  "name": "@pascaliske/schematics",
  "description": "Angular schematics collection for integrating setup tools like prettier and storybook.",
  "version": "0.0.8",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/schematics",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/schematics/issues"
  },
  "keywords": [
    "angular",
    "schematics"
  ],
  "schematics": "./dist/collection.json",
  "scripts": {
    "watch": "webpack --config webpack.config.ts --progress --mode development --watch",
    "build": "webpack --config webpack.config.ts --progress --mode production",
    "lint": "tslint --project tsconfig.json --fix",
    "check": "snyk test && snyk monitor",
    "format": "prettier --config .prettierrc --write \"**/*.{ts,md}\"",
    "changelog": "standard-changelog",
    "link": "yarn run build && yarn link",
    "unlink": "yarn unlink"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@angular-devkit/core": "^8.0.6",
    "@angular-devkit/schematics": "^8.0.6",
    "@schematics/angular": "^8.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@pascaliske/tsconfig": "^1.0.4",
    "@types/node": "^12.0.12",
    "@types/webpack": "^4.4.34",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "generate-json-webpack-plugin": "^0.3.1",
    "husky": "^3.0.0",
    "lint-staged": "^9.0.2",
    "prettier": "^1.18.2",
    "rxjs-tslint-rules": "^4.24.3",
    "snyk": "^1.190.0",
    "standard-changelog": "^2.0.11",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.5.2",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-node-externals": "^1.7.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
