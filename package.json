{
  "name": "@pascaliske/schematics",
  "description": "Angular schematics collection for integrating setup tools like prettier and storybook.",
  "version": "0.0.9",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/schematics",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/schematics/issues"
  },
  "keywords": [
    "angular",
    "schematics"
  ],
  "schematics": "./dist/collection.json",
  "scripts": {
    "watch": "webpack --config webpack.config.ts --progress --mode development --watch",
    "build": "webpack --config webpack.config.ts --progress --mode production",
    "lint": "tslint --project tsconfig.json --fix",
    "check": "snyk test && snyk monitor",
    "format": "prettier --config .prettierrc --write \"**/*.{ts,md}\"",
    "version": "bump --yes package.json",
    "changelog": "standard-changelog",
    "link": "yarn run build && yarn link",
    "unlink": "yarn unlink"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@pascaliske/prettier-config",
  "dependencies": {
    "@angular-devkit/core": "^9.0.5",
    "@angular-devkit/schematics": "^9.0.7",
    "@schematics/angular": "^9.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@pascaliske/prettier-config": "^1.0.2",
    "@pascaliske/tsconfig": "^1.0.4",
    "@types/node": "^13.9.3",
    "@types/webpack": "^4.41.8",
    "bump-cli": "^1.1.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "generate-json-webpack-plugin": "^0.3.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "rxjs-tslint-rules": "^4.29.2",
    "snyk": "^1.303.1",
    "standard-changelog": "^2.0.21",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
